<Window x:Class="Cecom.Vista.Multicentros.AlarmasM.VistaPrevia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cecom.Vista.Multicentros.AlarmasM"
        mc:Ignorable="d"
        Title="VistaPrevia" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        <WebBrowser Grid.Row="0" Name="webBrowser" />
        <Button Grid.Row="1" Content="Guardar" HorizontalAlignment="Right" Margin="0,10,50,10" Padding="20,0,20,0" Click="Cerrar_Click" />
    </Grid>
</Window>
