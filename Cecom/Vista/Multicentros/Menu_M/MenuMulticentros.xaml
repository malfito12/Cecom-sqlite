<Page x:Class="Cecom.Multicentros.MenuMulticentros"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cecom.Multicentros"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuMulticentros">

    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Ingreso Alarmas" Click="MenuItem_Click_1"/>
                <MenuItem Header="Registro Multicentro" Click="MenuItem_Click"/>
                <MenuItem Header="Reportes" Click="MenuItem_Click_2" />
            </Menu>
            <Frame x:Name="FrameMulticentros" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Page>
