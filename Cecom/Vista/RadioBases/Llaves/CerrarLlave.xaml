<Window x:Class="Cecom.Vista.RadioBases.Llaves.CerrarLlave"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cecom.Vista.RadioBases.Llaves"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
WindowStyle="SingleBorderWindow"
        Title="CerrarLlave" Height="450" Width="350">
    <Grid>
        <StackPanel Margin="20" Width="auto" x:Name="stackLlave">
            <Label Content="INFORMACION" HorizontalAlignment="Center"/>
            <Label Name="lbcodigo" />
            <Label Name="lbfecha" />
            <Label Name="lbhora" />
            <Label Name="lbsitio" />
            <Label Name="lbdepartamento" />
            <Label Name="lbempresa" />
            <Label Name="lbnombretec" />
            <Label Name="lbtelefono" />
            <Label Name="lbci" />
            <Label Name="lbresponsable" />
            <Label Name="lbmotivo" />
            <Label Name="lboperador" />
        </StackPanel>
    </Grid>
</Window>
