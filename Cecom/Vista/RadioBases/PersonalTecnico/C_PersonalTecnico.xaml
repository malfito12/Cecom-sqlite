<Window x:Class="Cecom.Vista.RadioBases.PersonalTecnico.C_PersonalTecnico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cecom.Vista.RadioBases.PersonalTecnico"
        mc:Ignorable="d"
        Title="C_PersonalTecnico" Height="350" Width="300">
    <Grid>
        <StackPanel Margin="20">
            <Label Content="Personal Tecnico" Margin="0,0,0,10" HorizontalAlignment="Center" FontSize="13" FontWeight="Bold" />
            <Label Content="Nombre Completo" />
            <TextBox x:Name="ptxt_nombre" />
            <Label Content="Cedula de Identidad" />
            <TextBox x:Name="ptxt_ci" />
            <Label Content="Telefono Celular" />
            <TextBox x:Name="ptxt_telefono" />
            <Label Content="Empresa" />
            <ComboBox x:Name="pcb_empresa"/>
            <Button Content="Guardar" Name="pbtn_guardar" Margin="0,20,0,0" Click="pbtn_guardar_Click" />
        </StackPanel>
    </Grid>
</Window>
