<Page x:Class="Cecom.RadioBases.MenuRadio"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cecom.RadioBases"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuRadio">

    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Registro Llaves" Click="MenuItem_Click_2" />
                <MenuItem Header="Personal Tecnico" Click="MenuItem_Click_1" />
                <MenuItem Header="Empresa" Click="MenuItem_Click" />
            </Menu>
            <Frame x:Name="FrameRadioBases" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Page>
